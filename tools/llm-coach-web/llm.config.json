{
  "providers": {
    "localResponses": {
      "type": "responses",
      "label": "Local SmolLM / Responses API",
      "maxTokensDefault": 4096
    },
    "openRouter": {
      "type": "openrouter_chat",
      "baseUrl": "https://openrouter.ai/api/v1",
      "defaultTier": "fast",
      "tiers": {
        "cheap": {
          "model": "google/gemini-flash-1.5",
          "maxTokens": 4096
        },
        "fast": {
          "model": "openai/gpt-4o-mini",
          "maxTokens": 4096
        },
        "quality": {
          "model": "openai/gpt-4.1",
          "maxTokens": 4096
        }
      },
      "temperature": 0.6,
      "topP": 0.95
    }
  },
  "fallbackPolicy": {
    "mode": "primary_local_then_openrouter",
    "openRouterTier": "fast",
    "triggers": {
      "networkError": true,
      "timeoutMs": 15000,
      "http5xx": true,
      "http429": true,
      "http4xx": false,
      "emptyOutput": true,
      "malformedResponse": true
    }
  }
}


